{
  "name": "alodev",
  "version": "1.0.0",
  "scripts": {
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build",
    "build:prod:stats": "ng build --stats-json --prod",
    "build:scully:prod": "npm run build:prod:stats && npm run scully && npm run scully:serve",
    "cypress": "$(npm bin)/cypress open",
    "scully": "scully --nw",
    "test": "cypress run --browser chrome",
    "test:firefox": "cypress run --browser firefox",
    "test:prebuild": "start-server-and-test start http-get://localhost:4200 test",
    "test:build": "start-server-and-test scully:serve http-get://localhost:4200 test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:build": "servor dist/alodev index.html 4200",
    "serve:firebase": "firebase serve --only hosting",
    "firebase:deploy": "firebase deploy --only hosting",
    "scully:serve": "servor dist/static index.html 4200 --reload"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.6",
    "@angular/cdk": "^9.1.0",
    "@angular/common": "^9.0.6",
    "@angular/compiler": "^9.0.6",
    "@angular/core": "^9.0.6",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "^9.0.6",
    "@angular/material": "^9.1.0",
    "@angular/platform-browser": "^9.0.6",
    "@angular/platform-browser-dynamic": "^9.0.6",
    "@angular/platform-server": "^9.0.6",
    "@angular/router": "^9.0.6",
    "@angular/service-worker": "^9.0.6",
    "@briebug/cypress-schematic": "^3.2.0",
    "@scullyio/init": "0.0.23",
    "@scullyio/ng-lib": "0.0.22",
    "@scullyio/scully": "0.0.83",
    "angular-google-tag-manager": "^1.1.1",
    "cookieconsent": "^3.1.0",
    "firebase-admin": "^11.4.1",
    "firebase-functions": "3.24.1",
    "ngx-cookieconsent": "^2.1.1",
    "rxjs": "6.5.4",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.6",
    "@angular/cli": "^9.0.6",
    "@angular/compiler-cli": "^9.0.6",
    "@angular/language-service": "^9.0.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@cypress/webpack-preprocessor": "4.1.3",
    "@types/jasmine": "^3.5.9",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.12.30",
    "codelyzer": "^5.1.2",
    "commitizen": "^4.0.4",
    "cypress": "^3.8.3",
    "html-minifier": "^4.0.0",
    "husky": "^4.2.5",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "prettier": "^2.0.4",
    "prettier-quick": "0.0.5",
    "pretty-quick": "^2.0.1",
    "reflect-metadata": "^0.1.12",
    "scully-disable-angular": "^2.2.1",
    "scully-minify-html": "^1.4.3",
    "servor": "^3.2.0",
    "start-server-and-test": "^1.10.11",
    "ts-loader": "6.2.1",
    "ts-node": "~8.3.0",
    "tslint": "~5.17.0",
    "typescript": "~3.7.5",
    "webpack-cli": "^3.3.11"
  }
}
